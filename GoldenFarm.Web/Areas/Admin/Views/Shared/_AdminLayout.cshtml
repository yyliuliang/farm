<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>农场物语</title>
    @Styles.Render("~/Content/sitecss")
    @Scripts.Render("~/bundles/sitejs")
    <script type="text/javascript">
        var Host = "";
    </script>

</head>
<body>
    @Html.Partial("_Header")
    <div class="subbanner-small subbanner-user" style="position: relative; z-index: 0; background: none;">
        <div class="subbanner-inner">
            <div class="container">
                <h1>农场物语</h1>
                <h2>管理平台</h2>
            </div>
        </div>
        <div class="backstretch" style="left: 0px; top: 0px; overflow: hidden; margin: 0px; padding: 0px; height: 120px; width: 1423px; z-index: -999998; position: absolute;">
            <img src="/content/images/banner-user02.png" style="position: absolute; margin: 0px; padding: 0px; border: none; width: 1423px; height: 177.875px; max-height: none; max-width: none; z-index: -999999; left: 0px; top: -28.9375px;">
        </div>
    </div>

    <div class="container clear">
        <div class="side">@Html.Partial("_AdminSideMenu")</div>
        <div class="main">@RenderBody()</div>
    </div>
        
        @Html.Partial("_Footer")
        @RenderSection("scripts", required: false)
</body>
</html>
